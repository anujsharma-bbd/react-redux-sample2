{
  "name": "nyc.hope.web.ui",
  "version": "0.0.1",
  "description": "New York City || Quarterly Count Application",
  "main": "index.js",
  "scripts": {
    "test": "jest -u --watchAll",
    "start": "webpack-dev-server --inline --progress --port 8080",
    "build": "rimraf dist && webpack --config config/webpack.prod.js --progress --profile --bail"
  },
  "keywords": [
    "test"
  ],
  "author": "Nagarro Pvt. Ltd.",
  "license": "NYC",
  "dependencies": {
    "classnames": "^2.2.5",
    "es6-promise": "^4.0.5",
    "immutable": "^3.8.2",
    "isomorphic-fetch": "^2.2.1",
    "rc-progress": "^2.0.3",
    "react": "^15.3.2",
    "react-addons-test-utils": "^15.4.2",
    "react-addons-transition-group": "^15.3.2",
    "react-confirm": "^0.1.14",
    "react-dnd": "^2.1.4",
    "react-dnd-touch-backend": "^0.3.5",
    "react-dom": "^15.3.1",
    "react-highcharts": "^10.0.0",
    "react-redux": "^4.0.0",
    "react-router": "^2.8.1",
    "react-scroll": "^1.4.4",
    "react-scrollbar": "^0.4.2",
    "react-select": "^1.0.0-rc.2",
    "react-toggle-button": "^2.1.0",
    "reactable": "^0.14.0",
    "redux": "^3.6.0",
    "tg-modal": "^0.5.0"
  },
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.2.5",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "copy-webpack-plugin": "3.0.1",
    "css-loader": "^0.25.0",
    "enzyme": "^2.7.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "0.8.5",
    "html-loader": "0.4.3",
    "html-webpack-plugin": "^2.24.0",
    "jest-cli": "^18.0.0",
    "jsdom": "^9.9.1",
    "mocha": "^3.2.0",
    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "^15.4.2",
    "react-test-renderer": "^15.4.1",
    "redux-mock-store": "^1.2.2",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "to-string-loader": "1.1.4",
    "url-loader": "0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.2",
    "webpack-merge": "0.14.0"
  },
  "babel": {
    "presets": [
      "react",
      "es2015"
    ]
  },
  "jest": {
    "testRegex": "/unit_test_cases/.*-test.jsx",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|svg)$": "<rootDir>/unit_test_cases/fileMock.js",
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "testEnvironment": "jsdom",
    "testPathIgnorePatterns": [
      "/__snapshots__/"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/app/**/*.{js,jsx}",
      "!**/app/*.js",
      "!**/node_modules/**",
      "!**/dist/**",
      "!**/app/assets/**",
      "!**/build/**"
    ]
  }
}
